# Host ports (published on 0.0.0.0 by Docker, i.e., reachable from LAN if firewall allows)
MYSQL_PORT=33306
PMA_PORT=8913

# MySQL root account
MYSQL_ROOT_PASSWORD=htl12!
# Allow root login from any host (required if you want to connect remotely with root)
MYSQL_ROOT_HOST=%

# Optional: default database created on first start (leave empty if not needed)
MYSQL_DATABASE=htl5b

# Optional: phpMyAdmin upload limit (e.g., 256M)
PMA_UPLOAD_LIMIT=256M
